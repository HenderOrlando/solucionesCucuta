{% block footer %}
    <div id="header">
    </div>
    <div class="uk-container uk-container-center uk-margin-top uk-margin-large-bottom">
        <h1 class="uk-text-center">Nuestros Clientes</h1>

        <div data-uk-slideset="{small: 2, medium: 4, large: 6, animation: 'scale', duration: 200}">
            <div class="uk-slidenav-position">
                <ul class="uk-slideset uk-grid uk-flex-center">
                    {% for cliente in clientes %}
                        <li>
                            <figure class="uk-overlay uk-overlay-hover">
                                <img class="uk-overlay-spin " src="/{{ cliente.getSrcBannerLateral }}" alt="{{ cliente.nombre }}">
                                <figcaption class="uk-overlay-panel uk-overlay-background uk-overlay-bottom uk-overlay-slide-bottom uk-text-center">
                                    <div>{{ cliente.descripcion }}</div>
                                </figcaption>
                                <a class="uk-position-cover" href="{{ path('cliente',{slug: cliente.slug}) }}"></a>
                            </figure>
                        </li>
                    {% endfor %}
                </ul>
                <a href="" class="uk-slidenav uk-slidenav-previous" data-uk-slideset-item="previous"></a>
                <a href="" class="uk-slidenav uk-slidenav-next" data-uk-slideset-item="next"></a>
            </div>
            <ul class="uk-slideset-nav uk-dotnav uk-flex-center">

            </ul>
        </div>
    </div>
{% endblock %}
