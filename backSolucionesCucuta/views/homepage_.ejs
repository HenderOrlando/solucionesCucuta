    <a href="" class="uk-navbar-brand">
      <img src="<%= brand.src %>" alt="<%= brand.nombre %>">
    </a>

    <div class="uk-dropdown uk-dropdown-small uk-dropdown-width-3">
      <ul class="uk-nav uk-nav-dropdown uk-grid uk-dropdown-grid">
        <% _.each(brand.subnavs, function(subnav){ %>
        <li class="uk-width-1-3">
          <a href="<%= subnav.href %>">
            <%=: __(subnav.nombre)|capitalize %>
          </a>
        </li>
        <% }) %>
      </ul>
    </div>
  </div>
  <div class="uk-navbar-flip uk-width-small-1-1 uk-width-medium-5-6">
    <ul class="uk-navbar-nav uk-width-small-1-1 uk-text-center">
      <% _.each(navs, function(nav, i){ %>
      <% if (nav.li){ %>
