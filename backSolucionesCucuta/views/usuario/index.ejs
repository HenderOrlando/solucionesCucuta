    <% _.each(banners, function(banner){ %>
    <li>
      <img src="<%= banner.srcBase %><%= banner.src %>" alt="<%= banner.nombre %>">
      <div class="uk-overlay-panel uk-overlay-background uk-overlay-right uk-overlay-slide-right uk-width-1-4">
        <h3><%= banner.nombre %></h3>
        <p>
          <%= banner.descripcion %>
        </p>
        <button href="<%= banner.href %>" class="uk-button uk-button-primary"><%= __('conocer mÃ¡s') %></button>

      </div>
    </li>

    <% }) %>
  </ul>
  <a href="" class="uk-slidenav uk-slidenav-contrast uk-slidenav-previous" data-uk-slideshow-item="previous"></a>
  <a href="" class="uk-slidenav uk-slidenav-contrast uk-slidenav-next" data-uk-slideshow-item="next"></a>
  <ul class="uk-dotnav uk-dotnav-contrast uk-position-bottom uk-text-center uk-flex uk-flex-center uk-flex-medium">
    <% _.each(banners, function(banner, i){ %>
    <li data-uk-slideshow-item="<%= i %>"><a href=""></a></li>
    <% }) %>
  </ul>
</div>


<div class="content uk-grid">
  <div class="uk-container-center uk-width-small-5-6 uk-width-medium-7-8 uk-width-large-9-10 uk-width-xlarge-11-12">
    <div class="uk-grid">
      <div class="uk-grid uk-width-small-4-6 uk-width-medium-6-8 uk-width-large-8-10 uk-width-xlarge-10-12">
        <% _.each(articles, function(article){ %>
        <article class="uk-article <% article.fullwidth?'uk-width-1-1':'' %>"
        <% _.each(article.attrs, function(attr){ %>
        <%= attr.name %><%= '=' %>"<%= attr.value %>"
        <% }) %>>
        <h1 class="uk-article-title" data-uk-sticky="{boundary: true, top:70, animation: 'uk-animation-slide-top'}">
          <%= article.title %>
        </h1>
        <p class="uk-article-meta">
          <% _.each(article.metas, function(meta){ %>
          <%=: __(meta.name)|capitalize %> <%= meta.value %>"
          <% }) %>
        </p>
        <% if (article.subtitle){ %>
        <p class="uk-article-lead">
          <%= article.subtitle %>
        </p>
        <% } %>
        <% _.each(article.content, function(content){ %>
        <p>
          <%= content %>
        </p>
        <% }) %>
        <% if (article.footer){ %>
        <hr class="uk-article-divider">
        <p class="uk-article-foot">
          <%= article.footer %>
        </p>
        <% } %>

        </article>


        <% }) %>
      </div>
      <div class="uk-publicity uk-width-small-2-6 uk-width-medium-2-8 uk-width-large-2-10 uk-width-xlarge-2-12">
        <% _.each(figures, function(figure){ %>
        <figure class="uk-overlay uk-overlay-hover"  data-uk-scrollspy="{cls:'uk-animation-slide-right', repeat: true, delay: 200}">
          <img class="uk-overlay-scale lazy" src="<%= figure.srcBase?figure.srcBase:'' %><%= figure.src %>" alt="<%= figure.nombre %>" width="640" height="480">
          <figcaption class="uk-overlay-panel uk-flex uk-flex-center uk-flex-middle uk-text-center uk-overlay-fade uk-overlay-background">
            <%= figure.nombre %>
          </figcaption>
          <a class="uk-position-cover" href="<%= figure.href %>"></a>
        </figure>

        <% }) %>
      </div>
    </div>
  </div>
</div>


